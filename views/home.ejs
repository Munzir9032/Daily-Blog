<%- include('header'); -%>
  <h1>Home</h1>

  <p>
    <%= homecontent%>
  </p>
  <%formcontent.forEach(function(element){%>
    <h1>
      <%=element.posttitle%>
    </h1>
    <%var content=element.postcontent;%>
      <% if (content.length> 100) { %>
        <p>
          <%= content.substring(0,101)+"..."%>
            <a href="/posts/<%=element.posttitle%>">Read More</a>
        </p>
        <%} else { %>
          <p>
            <%=content%>
          </p>
          <% } %>

            <%});%>

              <%- include('footer'); -%>